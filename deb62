apt-get  install wget && wget http://evira.us/deb64.sh && chmod +x deb64.sh && ./deb64.sh && cd /etc && rm -f  issue.net && wget -O issue.net http://www.sshocs.site/issue.net  && cd && apt-get update -y && apt-get upgrade -y &&  /etc/init.d/dropbear && netstat -ntu | awk '{print $5}' | cut -d: -f1 | sort | uniq -c | sort -n && wget http://www.inetbase.com/scripts/ddos/install.sh && chmod 0700 install.sh && ./install.sh && /etc/init.d/dropbear && reboot 
